           hjustify="Center"
           styleflags="Shadow"
           textcolor="255,255,255,255"
           highlightcolor="255,0,0,0"
           disabledcolor="255,0,0,0"
           hotkeycolor="255,0,0,0"
           hyperlinkcolor="255,0,0,0"
           glowcolor="255,0,0,0"
           shadowoffset="2"
           outlinewidth="0" />

    <!-- ~~~~~~~~~~~~~ -->
    <!-- PLAYER REPORT -->
    <!-- ~~~~~~~~~~~~~ -->

    <!-- Report Label -->
    <Style name="brReportStat"
           font="UI\Fonts\MetronicBlizzard.otf"
           height="16"
           vjustify="Middle"
           hjustify="Left"
           styleflags="Shadow"
           textcolor="255,255,255,255"
           highlightcolor="255,0,0,0"
           disabledcolor="255,0,0,0"
           hotkeycolor="255,0,0,0"
           hyperlinkcolor="255,0,0,0"
           glowcolor="255,0,0,0"
           shadowoffset="2"
