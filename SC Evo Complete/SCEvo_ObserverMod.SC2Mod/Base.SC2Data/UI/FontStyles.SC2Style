           font="UI\Fonts\MetronicBlizzard.otf"
           height="18"
           vjustify="Middle"
           hjustify="Right"
           styleflags=""
           textcolor="255,255,255,255"
           highlightcolor="255,255,255,255"
           disabledcolor="255,192,192,192"
           hotkeycolor="255,0,0,0"
           hyperlinkcolor="255,0,0,0"
           glowcolor="255,0,0,0"
           shadowoffset=""
