    <Style name="GraphData2" template="InternationalTemplate" height="17" vjustify="Middle" hjustify="Center" fontflags="HintStyleLight" styleflags="!Shadow" textcolor="255,255,255,255" highlightcolor="255,0,0,0" disabledcolor="255,0,0,0" hotkeycolor="255,0,0,0" hyperlinkcolor="255,0,0,0" glowcolor="255,0,0,0" shadowoffset="2"/>
    <!--
           Fonts:

           - MetronicBlizzard.otf
           - MetronicBlizzardBold.otf
           - MetronicProBold.ttf
           - ARUD.ttf
           - ARCrystalBold.ttf
           - Eurostile-Bol.otf
           - Eurostile-Reg.otf
           - EurostileExt-Med.otf
           - EurostileExt-Reg.otf
    -->



    <!-- ~~~~~~ -->
