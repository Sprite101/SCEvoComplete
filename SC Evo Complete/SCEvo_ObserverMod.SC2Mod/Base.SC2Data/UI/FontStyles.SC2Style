    <Style name="brTitle"
           font="UI\Fonts\MetronicBlizzard.otf"
           height="24"
           vjustify="Middle"
           hjustify="Center"
           styleflags=""
           textcolor="255,255,255,255"
           highlightcolor="255,0,0,0"
           disabledcolor="255,0,0,0"
           hotkeycolor="255,0,0,0"
           hyperlinkcolor="255,0,0,0"
           glowcolor="255,0,0,0"
           shadowoffset=""
           outlinewidth="0" />
           
    <!-- Battle Length Label -->
    <Style name="brBattleLength"
           font="UI\Fonts\MetronicBlizzard.otf"
           height="18"
           vjustify="Middle"
           hjustify="Center"
           styleflags=""
           textcolor="255,255,255,255"
           highlightcolor="255,0,0,0"
           disabledcolor="255,0,0,0"
           hotkeycolor="255,0,0,0"
           hyperlinkcolor="255,0,0,0"
           glowcolor="255,0,0,0"
           shadowoffset=""
           outlinewidth="0" />
           
    <!-- Powered By Label -->
    <Style name="brPoweredBy"
           font="UI\Fonts\bl.ttf"
           height="18"
           vjustify="Middle"
           hjustify="Center"
           styleflags=""
